{
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "insertPragma": false,
  "tabWidth": 2,
  "useTabs": false,
  "stylelintIntegration": true,
  "cssEnable": [
    "css",
    "less",
    "sass"
  ]
}
